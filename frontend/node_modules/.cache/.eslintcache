l={closeModal}\r\n          libCode={selectedLib}\r\n        />\r\n      )}\r\n    </Container>\r\n  );\r\n}\r\n\r\nconst Container = styled.div`\r\n  width: 100%;\r\n`;\r\n\r\nconst Item = styled.div`\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 9% 0 9% 8%;\r\n  border-bottom: 0.2px solid #bfbfbf;\r\n`;\r\n\r\nconst ItemName = styled.div`\r\n  font-weight: 400;\r\n  color: #5c5649;\r\n  font-size: 16px;\r\n  margin-right: 3%;\r\n`;\r\n\r\nconst ItemDist = styled.div`\r\n  font-weight: 400;\r\n  font-size: 13px;\r\n  color: #c2bfbb;\r\n`;\r\n","C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\Library\\LibraryModal.tsx",["426"],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\Home\\BestKeyword.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\apis\\library.ts",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\WriteText\\index.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\WriteText\\Comment.tsx",["427","428"],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\MeetingCreate\\index.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\Meeting\\index.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\MeetingRoom\\index.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\apis\\meeting.ts",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\Components\\Contents\\MeetingCover.tsx",[],[],"C:\\Users\\multicampus\\Desktop\\S08P22C104\\frontend\\src\\pages\\Meeting\\List.tsx",["429"],[],"import React, { useState, useEffect } from \"react\";\r\nimport styled from \"styled-components\";\r\nimport ListCard from \"./ListCard\";\r\nimport Button from \"./Button\";\r\nimport { getBooksAPI, getBookMeetingAPI } from \"../../apis/meeting\";\r\nimport { MeetingInfo, UserBook } from \"../../Store/Types\";\r\n\r\nexport default function List() {\r\n  c